{
    "name": "bengor-file/symfony-filesystem-bridge",
    "description": "Adapter to make File compatible with Symfony filesystem",
    "keywords": ["file", "adapter", "symfony", "filesystem", "ddd"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Beñat Espiña Diaz",
            "email": "benatespina@gmail.com",
            "homepage": "http://benatespina.com"
        },
        {
            "name": "Gorka Laucirica Ibarra",
            "email": "gorka.lauzirika@gmail.com",
            "homepage": "http://gorkalaucirica.net"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "bengor-file/file": "^0.2 || ^0.3",
        "symfony/filesystem": "^2.8 || ^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.11"
    },
    "scripts": {
        "cs": [
            "php-cs-fixer fix --config-file=.php_cs"
        ]
    },
    "autoload": {
        "psr-4": {
            "BenGorFile\\SymfonyFilesystemBridge\\": "src/BenGorFile/SymfonyFilesystemBridge/"
        },
        "exclude-from-classmap": ["/spec/"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
